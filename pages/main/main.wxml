<!--pages/main.wxml-->
<view class="userinfo">
  <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
  <text class="userinfo-welcome">欢迎</text>
  <text class="userinfo-nickname">{{userInfo.nickName}}</text>
  <!-- <text class="userinfo-nickname">Keung</text> -->
</view>

<view class="card">
  <view class="card-gray" >
    <text class="text-gray">添加你的订阅</text>
    <image class="button-gray" bindtap="addSubscribe" src="/res/img/add.png" mode="cover"></image>
  </view>
</view>

